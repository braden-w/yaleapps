---
import { courses, db } from '@repo/db/courses'
import Layout from '../layouts/Layout.astro'
import { CoursesDataTable } from './courses/_/CoursesDataTable'
const allCourses = await db.select().from(courses).limit(10)
---

<Layout title="Welcome to Astro.">
	<CoursesDataTable data={allCourses} client:load />
</Layout>
